<section class="orders light-section"> 
     <div class="container mx-auto pt-12">
       <h1 class="font-bold text-lg mb-4 mx-10"> All orders</h1>
     </div>
     <table class="w-full table-auto bg-white  mx-10 mr-10 ">
       <thead>
         <tr>
           <th class="px-4 py-2 text-left">Orders</th>
           <th class="px-4 py-2 text-left">Address</th>
           <th class="px-4 py-2 text-left">Phonenumber</th>
           <th class="px-4 py-2 text-left">Time</th>
         </tr>
       </thead>
       <tbody>
         <% if(orders.length)  { %>
         <% orders.forEach((order)=> { %>
          
         <tr>
           <td class="border px-4 py-2">
              <a class="link" href="#"><%= order._id %></a>
           </td>
           <td class="border px-4 py-2">
            <%= order.address %>
           </td>
           <td class="border px-4 py-2">
            <%= order.phone %>
          </td>
          <td class="border px-4 py-2">
              <%= moment(order.createdAt).format("MMM Do YY") %>
          </td>
<!-- moment().format('MMMM Do YYYY, h:mm:ss a') -->

         </tr>
         
         <% }) %>
         <% } else { %>
           <tr>
             <td class="p-4"> <span>No orders found</span> </td>
           </tr>

          <% } %>
       </tbody>

     </table>
</section>

